{
  "name": "linktr",
  "version": "1.4.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production remix vite:build",
    "bump-version": "standard-version --skip.commit=true --skip.tag=true",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "NODE_ENV=development remix vite:dev --host",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "prepare": "husky"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.17.4",
    "@remix-run/cloudflare-pages": "^2.17.4",
    "@remix-run/react": "^2.17.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "isbot": "^5.1.35",
    "lucide-react": "^0.575.0",
    "react": "^19.2.4",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.5.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260219.0",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^9.39.2",
    "@remix-run/dev": "^2.17.4",
    "@remix-run/serve": "^2.17.4",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^6.1.1",
    "wrangler": "^4.67.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "@remix-run/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@remix-run/dev": {
      "wrangler": "$wrangler"
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.57.1"
  }
}
